<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#eaf7c6"
    tools:context=".rate_app">

    <Button
        android:id="@+id/button12"
        android:layout_width="56dp"
        android:layout_height="58dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="11dp"
        android:background="@drawable/butten_side"
        android:drawableTop="@drawable/menu_bar1"
        android:paddingTop="15dp"
        android:onClick="sidpop"/>


    <RelativeLayout
        android:layout_width="347dp"
        android:layout_height="494dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="111dp"
        android:background="@drawable/button_circul"

        android:orientation="vertical">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="69dp"
                android:background="#2A26F1">

            </RelativeLayout>

            <EditText
                android:id="@+id/editText6"
                android:layout_width="263dp"
                android:layout_height="184dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginTop="248dp"
                android:layout_marginEnd="58dp"
                android:layout_marginRight="58dp"
                android:ems="10"
                android:inputType="textPersonName" />

            <EditText
                android:id="@+id/editText7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginTop="132dp"
                android:layout_marginEnd="112dp"
                android:layout_marginRight="112dp"
                android:ems="10"
                android:inputType="textPersonName" />
        </RelativeLayout>




        <TextView
            android:textColor="#fff"
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="21dp"
            android:layout_marginEnd="75dp"
            android:layout_marginRight="75dp"
            android:text="Write a Review"
            android:textSize="30dp"


            />

        <TextView

            android:id="@+id/textView3"
            android:layout_width="123dp"
            android:layout_height="34dp"
            android:layout_below="@+id/textView"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="203dp"
            android:layout_marginRight="203dp"
            android:hint="Enter Title Here:"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="260dp"
            android:layout_height="42dp"
            android:layout_below="@+id/textView3"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="89dp"
            android:layout_marginEnd="61dp"
            android:layout_marginRight="61dp"
            android:text="Enter Your Message (Optional):" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="43dp"
            android:layout_marginRight="43dp"
            android:layout_marginBottom="46dp"
            android:text="______________________________________" />

        <Button
            android:id="@+id/button7"
            android:layout_width="wrap_content"
            android:layout_height="34dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="456dp"
            android:layout_marginEnd="39dp"
            android:layout_marginRight="39dp"
            android:text="Submit" />


    </RelativeLayout>

    <Button
        android:id="@+id/button6"
        android:layout_width="177dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-4dp"
        android:layout_marginLeft="-4dp"
        android:layout_marginBottom="-6dp"
        android:text="My Profile"
        android:background="#ffffff"
        android:textAllCaps="false"/>


    <Button
        android:id="@+id/button9"
        android:layout_width="207dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-4dp"
        android:layout_marginRight="-4dp"
        android:layout_marginBottom="-7dp"
        android:background="#ffffff"
        android:text="My Downloads"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/button11"
        android:layout_width="209dp"
        android:layout_height="58dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="-17dp"
        android:background="#fff"
        android:text="Home"
        android:textAllCaps="false" />

</RelativeLayout>