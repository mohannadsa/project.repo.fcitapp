<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#eaf7c6"
    tools:context=".Openl">

    <TextView
        android:textColor="#000000"
        android:paddingTop="25dp"
        android:paddingBottom="20dp"
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Materials"
        android:textSize="30dp"
        android:textFontWeight="20" />

    <Button
        android:id="@+id/button6"
        android:layout_width="177dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-4dp"
        android:layout_marginLeft="-4dp"
        android:layout_marginBottom="-6dp"
        android:text="My Profile"
        android:background="#ffffff"
        android:textAllCaps="false"/>

    <Button
        android:id="@+id/button9"
        android:layout_width="207dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-4dp"
        android:layout_marginRight="-4dp"
        android:layout_marginBottom="-7dp"
        android:background="#ffffff"
        android:text="My Downloads"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/button11"
        android:layout_width="209dp"
        android:layout_height="58dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginBottom="-17dp"
        android:background="#fff"
        android:text="Home"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/button12"
        android:layout_width="56dp"
        android:layout_height="58dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="11dp"
        android:background="@drawable/butten_side"
        android:drawableTop="@drawable/menu_bar1"
        android:paddingTop="15dp"
        android:onClick="sidpop"/>

    <RelativeLayout
        android:layout_width="343dp"
        android:layout_height="103dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="410dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:background="#fff">

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="34dp"
            android:layout_marginEnd="143dp"
            android:layout_marginRight="143dp"
            android:text="SLIDES" />

        <Button
            android:id="@+id/button7"
            android:layout_width="90dp"
            android:layout_height="32dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="63dp"
            android:layout_marginEnd="6dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/downloadbutten"
            android:text="Download" />

        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="143dp"
            android:layout_height="match_parent"
            app:srcCompat="@drawable/slides" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="343dp"
        android:layout_height="103dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="226dp"
        android:layout_marginEnd="31dp"
        android:layout_marginRight="31dp"
        android:background="#fff">

        <Button
            android:id="@+id/fcittwt"
            android:layout_width="90dp"
            android:layout_height="32dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="61dp"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:background="@drawable/downloadbutten"
            android:text="Download" />

        <ImageView
            android:id="@+id/imageView20"
            android:layout_width="148dp"
            android:layout_height="102dp"
            app:srcCompat="@drawable/book" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="34dp"
            android:layout_marginEnd="143dp"
            android:layout_marginRight="143dp"
            android:text="BOOK" />

    </RelativeLayout>

    <TextView
        android:id="@+id/textView14"
        android:layout_width="313dp"
        android:layout_height="56dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="543dp"
        android:layout_marginEnd="50dp"
        android:layout_marginRight="50dp"
        android:text="Note* all files from books and slides are going to be Compressed and will be downloaded in .RAR"
        android:textStyle="bold" />

    <RelativeLayout
        android:layout_width="341dp"
        android:layout_height="43dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="105dp"
        android:layout_marginEnd="29dp"
        android:layout_marginRight="29dp"
        android:background="#fff">

        <Spinner
            android:id="@+id/spinner2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            />
    </RelativeLayout>


</RelativeLayout>



